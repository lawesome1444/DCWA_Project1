<h1>Add Manager</h1>

<form action="/managers/add" method="POST">

    <!-- Store ID -->
    <label>Manager ID</label>
    <input type="text" id="_id" name="_id" required minlength="4" maxlength="4" required/>
    <br/>
    <!-- Location -->
    <label>Name</label>
    <input type="text" id="name" name="name" required minlength="5" required/>
    <br/>
    <!-- Manager ID -->
    <label>Salary</label>
    <input type="number" id="salary" name="salary" min="30000" max="70000" required/>
    <br/>
    <button type="submit">Add Manager</button>
</form>

<!--Return to the Store Page-->
<a href="/managers">Cancel</a>